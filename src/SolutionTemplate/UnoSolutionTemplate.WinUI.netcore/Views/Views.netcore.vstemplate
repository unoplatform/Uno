<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
	<TemplateData>
		<Name>UnoQuickStart</Name>
		<Description>
		</Description>
		<ProjectType>CSharp</ProjectType>
		<ProjectSubType>
		</ProjectSubType>
		<SortOrder>1000</SortOrder>
		<CreateNewFolder>true</CreateNewFolder>
		<DefaultName>UnoQuickStart</DefaultName>
		<ProvideDefaultName>true</ProvideDefaultName>
		<LocationField>Enabled</LocationField>
		<EnableLocationBrowseButton>true</EnableLocationBrowseButton>
		<CreateInPlace>true</CreateInPlace>
		<Icon>__TemplateIcon.png</Icon>
		<Hidden>true</Hidden>
		<PreviewImage>__PreviewImage.png</PreviewImage>
	</TemplateData>
	<TemplateContent>
		<Project TargetFileName="$ext_safeprojectname$.csproj" File="UnoQuickStart.csproj" ReplaceParameters="true">
			<Folder Name="Assets" TargetFolderName="Assets">
			</Folder>
			<Folder Name="Strings" TargetFolderName="Strings">
				<Folder Name="en" TargetFolderName="en">
					<ProjectItem TargetFileName="Resources.resw" ReplaceParameters="true">Resources.resw</ProjectItem>
				</Folder>
			</Folder>
			<ProjectItem TargetFileName="App.cs" ReplaceParameters="true">App.cs</ProjectItem>
			<ProjectItem TargetFileName="AppResources.xaml" ReplaceParameters="true">AppResources.xaml</ProjectItem>
			<ProjectItem TargetFileName="MainPage.xaml" ReplaceParameters="true">mainpage.xaml</ProjectItem>
			<ProjectItem TargetFileName="MainPage.xaml.cs" ReplaceParameters="true">mainpage.xaml.cs</ProjectItem>
		</Project>
