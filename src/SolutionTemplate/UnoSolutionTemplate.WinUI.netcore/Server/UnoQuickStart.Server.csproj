<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<TargetFramework>$basetargetframework$-wasm1.0</TargetFramework>
		<Nullable>enable</Nullable>
		<TargetPlatformSupported>true</TargetPlatformSupported>
		<TargetPlatformVersion>1.0</TargetPlatformVersion>
	</PropertyGroup>

	$if$($UseWebAssembly$ == True)<ItemGroup>
		<ProjectReference Include="..\$ext_safeprojectname$.Wasm\$ext_safeprojectname$.Wasm.csproj" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Uno.Wasm.Bootstrap.Server" Version="7.0.0-dev.195" />
	</ItemGroup>$endif$

	<ItemGroup>
		<SupportedPlatform Include="wasm" />
		<SdkSupportedTargetPlatformVersion Include="1.0" />
	</ItemGroup>
</Project>
