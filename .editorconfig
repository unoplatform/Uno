root = true

[*]
tab_width = 4
insert_final_newline = true

[*.{xaml,xml,targets,props,csproj,cs,tt,java,ts,json}]
indent_style = tab
tab_width = 4
indent_size = 4

[*.js]
indent_style = space

[*.css]
indent_style = space
tab_width = 2
indent_size = 2

[*.{yml,yaml}]
indent_style = space
indent_size = 2

[*.md]
indent_style = space

